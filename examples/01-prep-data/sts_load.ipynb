   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def download_sts(url, dirpath):\n",
    "    zipfile = maybe_download(url, work_directory = dirpath)\n",
    "    unzipped = extract_sts(zipfile, target_dirpath = dirpath, tmode = \"r:gz\")\n",
    "    return zipfile, unzipped"
   ]
  },
